      <iTunesMetadata xmlns="http://music.apple.com/lyric-ttml-internal">
        <translations>
          <translation type="subtitle" xml:lang="en-US">
            <text for="L1">The light shines</text>
            <text for="L2">Illuminating the road</text>
            <text for="L3">My heart is touched</text>
            <text for="L4">As I watched you</text>
            <text for="L5">I reach my hand toward the sky</text>
            <text for="L6">I still don't know</text>
            <text for="L7">the answer</text>
            <text for="L8">And I wave my hand to you</text>

            <text for="L9">Let our voices reach</text>
            <text for="L10">the morning</text>
            <text for="L11">Even the darkest night will be repainted</text>
            <text for="L12">I won't let go</text>
            <text for="L13">Your hand, right now
              <span xmlns:ttm="http://www.w3.org/ns/ttml#metadata" ttm:role="x-bg" xmlns="http://www.w3.org/ns/ttml">(Your hand, right now)</span>
            </text>
            <text for="L14">Let's move forward together into tomorrow
              <span xmlns:ttm="http://www.w3.org/ns/ttml#metadata" ttm:role="x-bg" xmlns="http://www.w3.org/ns/ttml">(Let's move forward together into tomorrow)</span>
            </text>

            <text for="L15">The wind passes through
              <span xmlns:ttm="http://www.w3.org/ns/ttml#metadata" ttm:role="x-bg" xmlns="http://www.w3.org/ns/ttml">(The wind passes through)</span>
            </text>
            <text for="L16">The light of the city</text>
            <text for="L17">Morning changes</text>
            <text for="L18">with your smile
              <span xmlns:ttm="http://www.w3.org/ns/ttml#metadata" ttm:role="x-bg" xmlns="http://www.w3.org/ns/ttml">(with your smile)</span>
            </text>
            <text for="L19">With a hushed voice</text>
            <text for="L20">Deep in my heart</text>
            <text for="L21">I dream of tying the threads of tomorrow</text>

            <text for="L22">With a hushed voice</text>
            <text for="L23">I close my eyes</text>
            <text for="L24">and think of you</text>
            <text for="L25">I'll wait just a little longer</text>

            <text for="L26">Our voices will reach</text>
            <text for="L27">the morning</text>
            <text for="L28">Even the deepest night will be repainted</text>
            <text for="L29">I won't let go</text>
            <text for="L30">Your hand, tightly in mine</text>
            <text for="L31">Let's dash together into tomorrow</text>
          </translation>
        </translations>
        <songwriters>
          <songwriter>水燈</songwriter>
        </songwriters>
